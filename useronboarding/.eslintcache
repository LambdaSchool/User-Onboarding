\nuseEffect(()=>{\n  schema.isValid(formValues).then((valid)=>{\n    setDisabled(!valid)\n  })\n}, [formValues])\n\n\n\n  return (\n    <div className=\"App\">\n      <header>\n        <h1>New User Input</h1>\n      </header>\n      <FriendForm\n        values={formValues}\n        change={inputChange}\n        submit={formSubmit}\n        disabled={disabled}\n        errors={formErrors}\n      />\n\n      {friends.map((friend) => {\n        return <Friend key={friend.id} details={friend} />;\n      })}\n\n    </div>\n  );\n}\n\nexport default App;","/Users/Carson/Desktop/Lambda/User-Onboarding/useronboarding/src/components/Form.js",[],"/Users/Carson/Desktop/Lambda/User-Onboarding/useronboarding/src/components/Friend.js",[],["33","34"],"/Users/Carson/Desktop/Lambda/User-Onboarding/useronboarding/src/components/formSchema.js",[],{"ruleId":"35","replacedBy":"36"},{"ruleId":"37","replacedBy":"38"},{"ruleId":"39","severity":1,"message":"40","line":1,"column":8,"nodeType":"41","messageId":"42","endLine":1,"endColumn":13},{"ruleId":"43","severity":1,"message":"44","line":43,"column":31,"nodeType":"45","messageId":"46","endLine":43,"endColumn":32,"fix":"47"},{"ruleId":"35","replacedBy":"48"},{"ruleId":"37","replacedBy":"49"},"no-native-reassign",["50"],"no-negated-in-lhs",["51"],"no-unused-vars","'style' is defined but never used.","Identifier","unusedVar","rest-spread-spacing","Unexpected whitespace after spread operator.","SpreadElement","unexpectedWhitespace",{"range":"52","text":"53"},["50"],["51"],"no-global-assign","no-unsafe-negation",[1103,1104],""]